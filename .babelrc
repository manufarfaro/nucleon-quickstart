{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "78",
          "firefox": "64",
          "safari": "12",
          "edge": "17"
        },
        "modules": false,
        "corejs": "3.1.3",
        "useBuiltIns": "usage"
      }
    ],
    "@babel/preset-react",
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
